# Exemplo de variáveis de ambiente para Jenkins
# Copie para .jenkins.env e configure seus valores

# Docker Registry
DOCKER_REGISTRY=docker.io/seu-usuario
DOCKER_USERNAME=seu-usuario
DOCKER_PASSWORD=seu-token

# Git
GIT_REPO_URL=https://github.com/seu-usuario/api_produto.git
GIT_BRANCH=main

# Notificações (opcional)
SLACK_WEBHOOK_URL=
EMAIL_RECIPIENTS=dev@example.com

# Deploy
DEPLOY_HOST=production-server.com
DEPLOY_USER=deploy
DEPLOY_PATH=/opt/api_produto
